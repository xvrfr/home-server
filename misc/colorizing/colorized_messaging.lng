#!/usr/bin/env bash
#set -e

# Flush dictionary
declare -Ag $1

case "${2,,}" in
  "ru")
    declare -Ag $1='(
      ["info"]="Инфо"
      ["warn"]="Внимание"
      ["error"]="Ошибка"
    )'
  ;;

  "en")
    declare -Ag $1='(
      ["info"]="Info"
      ["warn"]="Warning"
      ["error"]="Error"
    )'
  ;;
esac
    
  
